{
  "name": "amazon-appstore-app-upload",
  "version": "1.0.0",
  "description": "Github Action for uploading android (.apk) app to Amazon Appstore",
  "main": "build/index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --files --respawn --pretty ./src/index.ts",
    "start": "node build/index.js",
    "clean": "rimraf build/",
    "build": "npm run clean && tsc && resolve-tspaths && npm ci --production"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "Ameya Jain (AmeyaJain-25)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.17.16",
    "@types/node-fetch": "^2.6.11",
    "resolve-tspaths": "^0.8.23",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "node-fetch": "^2.7.0"
  }
}
